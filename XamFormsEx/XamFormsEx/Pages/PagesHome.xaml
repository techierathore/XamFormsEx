<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XamFormsEx.Pages.PagesHome">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout VerticalOptions = "Center">
                <Button Text="Content Page" Clicked="OnContentClicked" />
                <Button Text="Tabbed Page" Clicked="OnTabbedClicked" />
                <Button Text="Carasoul Page" Clicked="OnCarsoulClicked" />
                <Button Text="Master Detail Page" Clicked="OnMasterClicked" />
                <Button Text="Stack Layout Example" Clicked="OnStackClicked" />
                <Button Text="Relative Layout" Clicked="OnRelativeClicked" />
                <Button Text="Absolute Layout" Clicked="OnAbsClicked" />
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>